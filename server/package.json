{
   "name": "snipify-server",
   "version": "1.0.0",
   "description": "api for snipify app",
   "main": "dist/server.js",
   "scripts": {
      "start": "NODE_ENV=production node dist/server.js",
      "start:background": "NODE_ENV=production pm2 start dist/server.js --name <edit>",
      "start:dev": "nodemon",
      "start:prod": "npm run build && npm run start:background",
      "build": "rimraf ./dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
      "test": "cross-env NODE_ENV=test jest --detectOpenHandles"
   },
   "homepage": "https://github.com/Mayowa-Ojo/snipify/#README",
   "repository": "github:Mayowa-Ojo/snipify",
   "keywords": [
      "typescript",
      "jwt",
      "nodejs",
      "oauth",
      "express"
   ],
   "engines": {
      "node": "14.x.x",
      "npm": "6.x.x"
   },
   "author": "Mayowa Ojo",
   "license": "ISC",
   "dependencies": {
      "@elastic/elasticsearch": "^7.11.0",
      "aws-sdk": "^2.831.0",
      "bcrypt": "^5.0.0",
      "class-validator": "^0.12.2",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^4.3.1",
      "http-status-codes": "^2.1.4",
      "jszip": "^3.5.0",
      "morgan": "^1.10.0",
      "nanoid": "^3.1.20",
      "node-fetch": "^2.6.1",
      "pg": "^8.5.1",
      "pg-hstore": "^2.3.3",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "typeorm": "^0.2.30",
      "uuid": "^8.3.2",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/cors": "^2.8.9",
      "@types/express": "^4.17.9",
      "@types/jest": "^26.0.20",
      "@types/morgan": "^1.9.2",
      "@types/node": "^14.14.9",
      "@types/node-fetch": "^2.5.8",
      "@types/pg": "^7.14.8",
      "@types/supertest": "^2.0.10",
      "@types/uuid": "^8.3.0",
      "@types/validator": "^13.1.0",
      "@types/winston": "^2.4.4",
      "cross-env": "^7.0.3",
      "jest": "^26.6.3",
      "nodemon": "^2.0.4",
      "supertest": "^6.0.1",
      "ts-jest": "^26.4.4",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "tscpaths": "0.0.9",
      "tslint": "^6.1.3",
      "typescript": "^4.1.2"
   }
}
